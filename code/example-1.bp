type bed(name, width) {
    Furniture("x" = 0, "y" = 0, "height"=100, "width"=width, "name" = name)
}

type bedroom( name) {
    bed("x" = 0, "y" = 0, name, 200)
    Room("x" = 200, "y" = 100, "height"=300, "width"=300, "name" = "roomies") {
        bed("x" = 2, "y" = 1, name, 100)
    }
}

Level("filepath" = "levels.svg", "width" = 1200, "height" = 700) {
    bedroom("x" = 200, "y" = 100, "ben")
}
